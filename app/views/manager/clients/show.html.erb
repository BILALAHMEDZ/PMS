<% title "-All Clients" %>
<div class="container">
  <table class="table table-striped" id="myTable">
    <thead>
      <tr>
        <th><strong>Name</strong></th>
        <th><strong>Phone</strong></th>
        <th><strong>Email</strong></th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td><%= @client.name %></td>
          <td><%= @client.phone %></td>
          <td><%= @client.email %></td>
        </tr>
    </tbody>
  </table>
  <div class="actions">
    <%= link_to "Back", :back, class: 'btn btn-success' %>
  </div>
</div>
